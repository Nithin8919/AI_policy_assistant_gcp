# Google Cloud Platform Configuration
# Copy this file to .env and fill in your actual values

# Your GCP Project ID
GCP_PROJECT_ID=your-gcp-project-id

# GCP Region/Location (e.g., us-central1, us-east1, asia-south1)
GCP_LOCATION=us-central1

# Optional: Service Account Key Path (for service account authentication)
# Leave empty to use Application Default Credentials (ADC) - recommended for local dev
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Vertex AI Configuration
VERTEX_AI_ENABLED=true

# Optional: Override default credentials location
# GOOGLE_APPLICATION_CREDENTIALS=$HOME/.config/gcloud/application_default_credentials.json
